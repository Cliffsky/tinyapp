<% include ../partials/head.ejs %>
<% include ../partials/header.ejs %>
  http://www.tinyapp.com/<%= short %>
  Views: <%= urlDb[short].views %>
  <form method="POST" action="/urls/<%= short %>?_method=UPDATE" style="padding-left:10px;">
    <input id="longURL" type="text" name="longURL" placeholder="<%= urlDb[short].long %>">
    <input id="submit" type="submit" value="Submit">
  </form>
  <div>
    <ul>
      <% for (var i = 0; i < urlDb[short].viewerIds.length; i += 1) { %>
      <li><%= urlDb[short].viewerIds[i] %></li>
      <% } %>
    </ul>